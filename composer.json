{
  "name": "engmohamedamer/yii2-starter",
  "description": "Yii2 Complete backend and restfull template",
  "keywords": [
    "yii2",
    "framework",
    "start",
    "cms",
    "application template",
    "yii2 advanced"
  ],
  "authors": [
    {
      "name": "Mohamed Amer",
      "email": "mohamed.amer2050@gmail.com"
    }
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/engmohamedamer/Yii2-Starter"
  },

  "minimum-stability": "dev",


  "require": {
    "php": ">=7.1.0",
    "ext-intl": "*",
    "yiisoft/yii2": "^2.0.13",
    "yiisoft/yii2-bootstrap4": "@dev",
    "yiisoft/yii2-swiftmailer": "^2.0.0",
    "yiisoft/yii2-authclient": "^2.0.0",
    "yiisoft/yii2-jui": "^2.0.0",
    "yii2-starter-kit/yii2-file-kit": "^2.0.0",
    "asofter/yii2-imperavi-redactor": ">=0.0.3@dev",
    "trntv/yii2-aceeditor": "^2.0",
    "trntv/probe": "^1.0",
    "trntv/yii2-glide": "^1.2",
    "trntv/yii2-datetime-widget": "dev-master@dev",
    "trntv/cheatsheet": "^0.1@dev",
    "trntv/yii2-command-bus": "^3.0",
    "intervention/image": "^2.1",
    "vlucas/phpdotenv": "^2.0",
    "npm-asset/admin-lte": "^2.0",
    "npm-asset/font-awesome": "^4.0",
    "npm-asset/html5shiv": "^3.0",
    "npm-asset/jquery-slimscroll": "^1.3",
    "npm-asset/flot": "^0.8@alpha",
    "symfony/process": "^4.0",
    "guzzlehttp/guzzle": "^6.0",
    "alexantr/yii2-elfinder": "^1.3",
    "trntv/sitemaped": "^0.1",
    "kartik-v/yii2-detail-view": "dev-master",
    "kartik-v/yii2-grid": "dev-master",
    "mootensai/yii2-enhanced-gii": "dev-master",
    "kartik-v/yii2-mpdf": "dev-master",
    "kartik-v/yii2-tree-manager": "dev-master",
    "pigochu/yii2-jquery-locationpicker": ">=0.2.0",
    "webvimark/multilanguage": "*",
    "newerton/yii2-fancybox-3": "*",
    "kartik-v/yii2-widget-depdrop": "@dev",
    "yiisoft/yii2-bootstrap": "^2.0@dev",
    "kartik-v/yii2-checkbox-x": "dev-master",
    "mihaildev/yii2-ckeditor": "*",
    "kartik-v/yii2-field-range": "dev-master",
    "kartik-v/yii2-widget-growl": "*"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "^2.0.0",
    "yiisoft/yii2-gii": "^2.0.0",
    "yiisoft/yii2-faker": "^2.0.0",
    "codeception/codeception": "2.4.0",
    "codeception/verify": "^0.3.1"
  },
  
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "suggest": {
    "trntv/yii2-debug-xhprof": "dev-master@dev"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "repositories": {
        "bower-asset/eve": {
          "type": "bower-vcs",
          "url": "https://github.com/adobe-webplatform/eve.git"
        }
      }
    }
  },

  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "archive": {
    "exclude": ["docs"]
  }
  
}
